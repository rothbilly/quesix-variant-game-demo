; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Position2D",
"class": "GridCreator",
"language": "GDScript",
"path": "res://Script/GridCreator.gd"
}, {
"base": "Node2D",
"class": "Player",
"language": "GDScript",
"path": "res://Script/Player.gd"
} ]
_global_script_class_icons={
"GridCreator": "",
"Player": ""
}

[application]

config/name="Quesix-V2"
run/main_scene="res://Scenes/Main.tscn"
boot_splash/bg_color=Color( 0.0588235, 0.054902, 0.0666667, 1 )

[autoload]

EventController="*res://Script/EventController.gd"

[display]

window/size/width=1152
window/size/height=2048
window/size/fullscreen=true
window/size/test_width=576
window/size/test_height=1024
window/dpi/allow_hidpi=true
window/handheld/orientation="portrait"
window/stretch/mode="2d"
window/stretch/aspect="keep_width"

[rendering]

environment/default_clear_color=Color( 0.121569, 0.0941176, 0.160784, 1 )
environment/default_environment="res://default_env.tres"
