[application]

config/name="Quesix-V2"
run/main_scene="res://Scenes/Levels/Main.tscn"
boot_splash/bg_color=Color( 0.0588235, 0.054902, 0.0666667, 1 )

[autoload]

EventController="*res://Script/EventController.gd"

[display]

window/size/width=1152
window/size/height=2048
window/size/always_on_top=true
window/size/test_width=576
window/size/test_height=1024
window/dpi/allow_hidpi=true
window/handheld/orientation="portrait"
window/stretch/mode="2d"
window/stretch/aspect="keep_width"

[mono]

project/assembly_name="Quesix-V2"

[rendering]

quality/driver/driver_name="GLES2"
vram_compression/import_etc=true
environment/default_clear_color=Color( 0.121569, 0.0941176, 0.160784, 1 )
environment/default_environment="res://default_env.tres"
